<?xml version="1.0"?>
<extension key="uk.artfulrobot.civicrm.mergeinfo" type="module">
  <file>mergeinfo</file>
  <name>Merge Info</name>
  <description>In a merge, if you select a contact with subtype as the
    duplicate, and there's custom data applied to that subtype, the custom data
    is lost since the remaining contact is not of that subtype. This extension
    provides some warnings about this to avoid data loss.</description>
  <license>GPL-3.0+</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/uk.artfulrobot.civicrm.mergeinfo</url>
    <url desc="Documentation">https://github.com/artfulrobot/uk.artfulrobot.civicrm.mergeinfo</url>
    <url desc="Support">https://github.com/artfulrobot/uk.artfulrobot.civicrm.mergeinfo</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2018-03-23</releaseDate>
  <version>1.0</version>
  <develStage>live</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Mergeinfo</namespace>
  </civix>
</extension>
